{
    "html": {
        "mandatory": true,
        "attrs": {
            "mip": {
                "mandatory": true
            }
        },
        "duplicate": true
    },

    "meta": {
        "duplicate": [{
            "name": "/^viewport/"
        }, {
            "http-equiv": "/Content-Type/i"
        }, {
            "charset": "/^(utf)|(UTF-8)/"
        }],
        "attrs": {
            "content": [{
                "match": {
                    "name": "viewport"
                },
                "properties": {
                    "width": "device-width",
                    "initial-scale": "1"
                }
            }, {
                "match": {
                    "http-equiv": "X-UA-Compatible"
                },
                "properties": {
                    "IE": "edge"
                }
            }]
        },
        "mandatory_or": [{
            "http-equiv": "/Content-Type/i",
            "content": "/charset=(utf)|(UTF)-8/"
        }, {
            "charset": "/^(utf)|(UTF-8)/"
        }],
        "mandatory": {
            "name": "viewport",
            "content": "width=device-width,minimum-scale=1,initial-scale=1"
        },
        "mandatory_parent": "head"
    },
    "link": {
        "mandatory": [{
            "rel": "/^(miphtml)|(standardhtml)$/"
        }, {
            "rel": "/^stylesheet$/",
            "href": "/^http(s)?:\/\/(mipcache.bdstatic.com)|(m.baidu.com\/static\/ala\/sf\/static\/)/"
        }],

        "mandatory_parent": "head",
        "duplicate": [{
            "rel": "/^(miphtml)|(standardhtml)$/"
        }, {
            "rel": "/^stylesheet$/",
            "href": "/^http(s)?:\/\/(mipcache.bdstatic.com)|(m.baidu.com\/static\/ala\/sf\/static\/)/"
        }]
    },
    "script": {
        "mandatory": {
            "src": "/^(http(s)?:)?\/\/(mipcache.bdstatic.com\/static\/mipmain)|(m.baidu.com\/static\/ala\/sf\/static\/)/"
        },
        "mandatory_or": [{
            "type": "/^(text/javascript)?/",
            "src": "/^(http(s)?:)?\/\/(mipcache.bdstatic.com)|(m.baidu.com\/static\/ala\/sf\/static\/)/"
        }, {
            "type": "/^(application\/(ld\\+)?json)$/"
        }]
    },
    "mip-input": {
        "mandatory_ancestor": "mip-form",
        "attrs": {
            "name": {
                "mandatory": true
            }
        }
    },


    
    "img": {
        "disallow": true
    },
    "video": {
        "disallow": true
    },
    "audio": {
        "disallow": true
    },
    "frame": {
        "disallow": true
    },
    "frameset": {
        "disallow": true
    },
    "object": {
        "disallow": true
    },
    "param": {
        "disallow": true
    },
    "applet": {
        "disallow": true
    },
    "embed": {
        "disallow": true
    },
    "form": {
        "disallow": true
    },
    "input": {
        "disallow": true
    },
    "textarea": {
        "disallow": true
    },
    "select": {
        "disallow": true
    },
    "option": {
        "disallow": true
    },





    "a": {
        "attrs": {
            "href": {
                "mandatory": true,
                "value": "/^((http(s)?:)?\/\/)|#/"
            },
            "target": {
                "mandatory": true,
                "value": "/^_blank$/"
            }
        }
    },
    "mip-img": {
        "attrs": {
            "src": {
                "mandatory": true,
                "value": "/^(http(s)?:)?\/\//"
            }
        }
    },
    "mip-pix": {
        "attrs": {
            "src": {
                "mandatory": true,
                "value": "/^(https:\/\/)|(http:\/\/*\\?)/"
            }
        }
    },
    "mip-video": {
        "attrs": {
            "src": {
                "mandatory": true,
                "value": "/^https:\/\//"
            },
            "poster": {
                "mandatory": true,
                "value": "/^http(s)?:\/\//"
            }
        }
    },
    "mip-carousel": {
        "attrs": {
            "width": {
                "mandatory": true,
                "value": "/^\\d{1,4}$/"
            },
            "height": {
                "mandatory": true,
                "value": "/^\\d{1,4}$/"
            }
        }
    },
    "mip-iframe": {
        "attrs": {
            "width": {
                "mandatory": true,
                "value": "/^\\d{1,4}$/"
            },
            "height": {
                "mandatory": true,
                "value": "/^\\d{1,4}$/"
            }
        }
    },
    "mip-appdl": {
        "attrs": {
            "tpl": {
                "mandatory": true,
                "value": "/^imageText$/"
            },
            "src": {
                "mandatory": true,
                "value": "/^http(s)?:\/\//"
            },
            "texttip": {
                "mandatory": true
            },
            "postiontye": {
                "mandatory": true,
                "value": "/^(static)|(fixed)$/"
            }
        }
    },
    "mip-audio": {
        "attrs": {
            "src": {
                "mandatory": true,
                "value": "/^https:\/\//"
            }
        }
    },
    "mip-stats-bidu": {
        "attrs": {
            "token": {
                "mandatory": true
            }
        }
    },
    "mip-form": {
        "attrs": {
            "method": {
                "mandatory": true
            },
            "url": {
                "mandatory": true
            }
        }
    },
    "mip-link": {
        "attrs": {
            "href": {
                "mandatory": true
            }
        }
    },
    "mip-ad": {
        "attrs": {
            "type": {
                "mandatory": true,
                "value": "/^ad\\-(baidu)|(comm)|(qwang)/"
            }
        }
    },
    "mip-embed": {
        "attrs": {
            "type": {
                "mandatory": true,
                "value": "/^ad\\-(baidu)|(comm)|(qwang)/"
            }
        }
    },
    "head": {
        "mandatory": true,
        "mandatory_parent": "html"
    },
    "body": {
        "mandatory": true,
        "mandatory_parent": "html"
    },
    "style": {
        "attrs": {
            "mip-custom": {
                 "mandatory": true
            }
        },
        "mandatory_parent": "head"
    }
    
}
